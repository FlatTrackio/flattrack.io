{
  "name": "flattrack.io",
  "version": "0.0.3",
  "description": "FlatTrack website",
  "author": "Caleb Woodbine <calebwoodbine.public@gmail.com>",
  "private": true,
  "scripts": {
    "start": "go run src/backend/main.go",
    "start:frontend": "vue-cli-service serve",
    "build:all":"npm run build:frontend && npm run build:api",
    "build:frontend": "vue-cli-service build --dest dist src/frontend/main.ts",
    "build:api": "go build -o flattrackio src/backend/main.go",
    "test:unit": "vue-cli-service test:unit --timeout 10000",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e-api": "ginkgo test/api/e2e",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "buefy": "^0.8.6",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-e2e-cypress": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-unit-mocha": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "@vue/cli-service": "4.0.0",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  }
}
